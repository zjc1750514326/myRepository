<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.EmployeeMapper" >
  <resultMap id="BaseResultMap" type="empty.Employee" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="empName" property="empname" jdbcType="VARCHAR" />
    <result column="empJob" property="empjob" jdbcType="VARCHAR" />
    <result column="empPs" property="empps" jdbcType="INTEGER" />
  </resultMap>

  <select id="select" resultMap="BaseResultMap" parameterType="empty.Employee">
    SELECT empName FROM employee
    WHERE empName = #{empname} AND empPs = #{empps}
  </select>

</mapper>