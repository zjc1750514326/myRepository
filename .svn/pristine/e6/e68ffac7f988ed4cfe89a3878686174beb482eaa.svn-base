<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.EmployeeMapperCustom" >
  	<sql id="query_employee_where">
	  	<if test="employeeCustom!=null">
	  		<if test="employeeCustom.empname!=null and employeeCustom.empname!=''">
	  			employee.empName like '%${employeeCustom.empname}%';
	  		</if>
	  	</if>
    </sql>
  
  <!-- 查找菜单列表 -->
  <select id="findEmployeeList" parameterType="empty.EmployeeQueryVo" resultType="empty.EmployeeCustom">
	  	SELECT * FROM employee
	  	<where>
	  		<include refid="query_employee_where"></include>
	  	</where>
  </select>
</mapper>